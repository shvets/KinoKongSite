source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/shvets/Specs.git'

use_frameworks!

target 'KinoKongSite_iOS' do
  platform :ios, '10.0'

  podspec :path => 'KinoKongSite.podspec'

  target 'KinoKongSite_iOSTests' do
    inherit! :search_paths
  end
end

target 'KinoKongSite_tvOS' do
  platform :tvos, '10.10'

  podspec :path => 'KinoKongSite.podspec'

  target 'KinoKongSite_tvOSTests' do
    inherit! :search_paths
  end
end

